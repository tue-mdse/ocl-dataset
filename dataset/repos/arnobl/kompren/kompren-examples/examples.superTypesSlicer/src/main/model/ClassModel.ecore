<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ex" nsURI="http://kompren/examples.classModel" nsPrefix="ex">
  <eSubpackages name="classModel" nsURI="http://kompren/examples.classModel/ex/classModel"
      nsPrefix="classModel">
    <eClassifiers xsi:type="ecore:EClass" name="ClassModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="classes" upperBound="-1"
          eType="#//classModel/Clazz" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Clazz">
      <eStructuralFeatures xsi:type="ecore:EReference" name="superClasses" upperBound="-1"
          eType="#//classModel/Clazz"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
