      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Returns a unqiue string representing the task. According to the processing stage of the graph, this string might contain grouping and traversal specific informations."/>
      </eAnnotations>
    </eOperations>
    <eOperations name="fitsToGroupingCriterionOf" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Check whether tha (current) tasks grouping criterion 'fits' to the grouping criterion of the given task. (I.e. returns true if there are at least one record in common.)"/>
