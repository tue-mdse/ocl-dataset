      <details key="documentation" value="The task class represents a data processing step. It is independed of a concrete implementation. The implementation is provided by the Tool. The task object represents the nodes of the graph (dag and mxgraph)."/>
    </eAnnotations>
    <eOperations name="readTask">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
