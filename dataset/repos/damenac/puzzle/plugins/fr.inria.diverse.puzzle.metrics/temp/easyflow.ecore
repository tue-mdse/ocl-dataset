        <details key="documentation" value="Evaluates the JEXL and returns the evaluation result."/>
      </eAnnotations>
      <eParameters name="map">
        <eGenericType eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EMap">
