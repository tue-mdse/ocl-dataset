      <eStructuralFeatures xsi:type="ecore:EReference" name="locatedAt" lowerBound="1"
          eType="#//providersite/SiteLocation" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="offeredResourcesList"
