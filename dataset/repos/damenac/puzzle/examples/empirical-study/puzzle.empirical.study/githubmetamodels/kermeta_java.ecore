          <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="To be written ################################"/>
          </eAnnotations>
        </eStructuralFeatures>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="IntegerLiteral" eSuperTypes="#//language/behavior/Literal">
