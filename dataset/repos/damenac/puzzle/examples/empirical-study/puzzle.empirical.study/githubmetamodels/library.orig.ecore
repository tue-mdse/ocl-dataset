      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/Javascript">
        <details key="Javascript" value="var name = this.name;&#xA;var pos = name.indexOf(' ');&#xA;return pos &lt; 0 ? null : name.substring(0, pos);"/>
      </eAnnotations>
