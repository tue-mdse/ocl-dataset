    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/Javascript">
      <details key="nameWithLetters" value="var i;&#xA;var s = this.name;&#xA;for (i = 0; s != null &amp;&amp; i &lt; s.length; i++) {&#xA;  var c = s.charAt(i);&#xA;  if (! (java.lang.Character.isLetter(c) || c == ' ')) {&#xA;    return false;&#xA;  }&#xA;}&#xA;return true;&#xA;"/>
