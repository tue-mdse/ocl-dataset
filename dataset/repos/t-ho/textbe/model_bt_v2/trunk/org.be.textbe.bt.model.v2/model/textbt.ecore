            <details key="documentation" value="Wait for event e internally (from system)"/>
          </eAnnotations>
        </eClassifiers>
        <eClassifiers xsi:type="ecore:EClass" name="ExternalInput" eSuperTypes="#//component/behavior/event/Input">
