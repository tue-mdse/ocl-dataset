<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Paper" nsURI="http:///Paper.ecore" nsPrefix="Paper">
  <eClassifiers xsi:type="ecore:EEnum" name="Sex">
    <eLiterals name="male" literal="male"/>
    <eLiterals name="female" value="1" literal="female"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="User">
    <eOperations name="UpdateGender">
      <eParameters name="gender" eType="#//Sex"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Gender" eType="#//Sex"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="UserLoc" lowerBound="1"
        eType="#//Location"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Location"/>
</ecore:EPackage>
