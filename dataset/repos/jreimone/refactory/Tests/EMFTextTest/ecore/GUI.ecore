<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="gui"
    nsURI="http://gui.org" nsPrefix="gui">
  <eClassifiers xsi:type="ecore:EClass" name="UIElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="Button" eSuperTypes="#//UIElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="Label" eSuperTypes="#//UIElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="TextField" eSuperTypes="#//UIElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="Panel" eSuperTypes="#//UIElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//UIElement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ImageLabel" eSuperTypes="#//Label">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="imageURI" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
