[comment encoding = UTF-8 /]
[module textProcessing('http://www.eclipse.org/uml2/4.0.0/UML')]

[**
 * This template defines the display format
 * @param aClass
*/]
[template public generateTextProcessingLine(s : String)]
[s.replaceAll('\\n', '')/]
[/template]

[template public generateTextProcessingBlock(s : String)]
[s.replaceAll('[\\n]+', '\n')/]
[/template]
