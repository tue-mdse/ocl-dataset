[comment encoding = UTF-8 /]
[module queryComplexUseCase('http://www.eclipse.org/emf/2002/Ecore')/]

[template public main(eClass : EClass)]
[file (eClass.name + '.txt', false, 'UTF-8')][Sequence{bidule('bidule') + chose('chose'), chose('chose'), machin('machin')}->sep(gnagna('gnagna'))/][/file]
[/template]

[query public bidule(s : String) : String = s/]

[query public chose(s : String) : String = s/]

[query public machin(s : String) : String = s/]

[query public gnagna(s : String) : String = s/]