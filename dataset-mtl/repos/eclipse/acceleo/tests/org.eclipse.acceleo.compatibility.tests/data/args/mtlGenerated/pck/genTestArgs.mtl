[module genTestArgs('http://www.eclipse.org/uml2/2.0.0/UML')/]

[template public generate(class : Class)]
[comment This template is called by the main module file /]
[file ('test.txt', false))]
== Sample of text ==

=Start=

[argScript1('arg0', 'arg1')/]

[argScript2('arg0', 'arg1')/]

[argScript3('arg0', 'arg1', 'arg2')/]

[argScript4('arg0', 'arg1')/]

=End=
[/file]
[/template]

[template public argScript1(class : Class, args0 : OclAny, args1 : OclAny)]
arg 0: [args0/] / arg 1: [args1/]
[/template]

[template public argScript2(class : Class, args0 : OclAny, args1 : OclAny)]
arg 1: [args1/] / arg 0: [args0/]
[/template]

[template public argScript3(class : Class, args0 : OclAny, args1 : OclAny, args2 : OclAny)]
arg 0: [args0/] / arg 1: [args1/] / arg 2: [args2/]
[/template]

[template public argScript4(class : Class, args0 : OclAny, argsone() : OclAny)]
first arg : [args0/]  second arg : [argsone()/]
[/template]

[template public one(class : Class)]
1
[/template]


