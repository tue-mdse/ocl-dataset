		<impala.version>1.0</impala.version>
		<impala.plugin.version>1.0-M2</impala.plugin.version>
	</properties>
	<name>[aModel.generateSpecificProjectFolder('')/]-build Parent build project for multi-module Impala build</name>
    <modules>
		<module>../[aModel.generateSpecificProjectFolder('-main')/]</module>
		<module>../[aModel.generateSpecificProjectFolder('-dto')/]</module>
		<module>../[aModel.generateSpecificProjectFolder('-hibernate')/]</module>
		<module>../[aModel.generateSpecificProjectFolder('-dao')/]</module>
		<module>../[aModel.generateSpecificProjectFolder('-service')/]</module>
