    <eStructuralFeatures xsi:type="ecore:EReference" name="synchroeOpposite" upperBound="-1"
        eType="#//Synchro" eOpposite="#//Synchro/synchro"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InstantaneousInstruction" abstract="true"
      eSuperTypes="#//Instruction"/>
</ecore:EPackage>
