[module rteBswApiHeader('http://nces.is.nagoya-u.ac.jp/a-rte/rte/module')]

[import jp::ac::nagoya_u::is::nces::a_rte::codegen::rte::common::fileUtil /]

[template public genRteBswApiHeader(aRteHeader : RteBswApiHeader)]
[file (aRteHeader.fileName, false, 'ASCII')]
	[aRteHeader.genFileHeader()/]
	[aRteHeader.genBuiltinBswApiAliasDefinitions()/]
	[aRteHeader.genIocApiAliasDefinitions()/]
	[aRteHeader.genFileFooter()/]
[/file]
[/template]

[template private genFileHeader(aRteHeader : RteBswApiHeader)]
[aRteHeader.genFileHeaderComment()/]

[aRteHeader.genGuardDeclarationStart()/]
[/template]

[template private genFileFooter(aRteHeader : RteBswApiHeader)]
[aRteHeader.genGuardDeclarationEnd()/]
[/template]

[template private genBuiltinBswApiAliasDefinitions(aRteHeader : RteBswApiHeader)]
/*
 * RTE API Definitions
 */
#define Rte_Call_Bsw_TerminateTask TerminateTask
#define Rte_Call_Bsw_SuspendAllInterrupts SuspendAllInterrupts
#define Rte_Call_Bsw_ResumeAllInterrupts ResumeAllInterrupts
#define Rte_Call_Bsw_SuspendOSInterrupts SuspendOSInterrupts
#define Rte_Call_Bsw_ResumeOSInterrupts ResumeOSInterrupts
#define Rte_Call_Bsw_GetResource GetResource
#define Rte_Call_Bsw_ReleaseResource ReleaseResource
#define Rte_Call_Bsw_SetEvent SetEvent
#define Rte_Call_Bsw_ActivateTask ActivateTask
#define Rte_Call_Bsw_ClearEvent ClearEvent
#define Rte_Call_Bsw_GetEvent GetEvent
#define Rte_Call_Bsw_WaitEvent WaitEvent
#define Rte_Call_Bsw_CallTrustedFunction CallTrustedFunction
#define Rte_Call_Bsw_GetTaskID GetTaskID
#define Rte_Call_Bsw_CheckTaskMemoryAccess CheckTaskMemoryAccess
#define Rte_Call_Bsw_GetSpinlock GetSpinlock
#define Rte_Call_Bsw_ReleaseSpinlock ReleaseSpinlock

#define Rte_Call_Bsw_Com_SendSignal Com_SendSignal
#define Rte_Call_Bsw_Com_ReceiveSignal Com_ReceiveSignal
#define Rte_Call_Bsw_Com_UpdateShadowSignal Com_UpdateShadowSignal
#define Rte_Call_Bsw_Com_ReceiveShadowSignal Com_ReceiveShadowSignal
#define Rte_Call_Bsw_Com_SendSignalGroup Com_SendSignalGroup
#define Rte_Call_Bsw_Com_ReceiveSignalGroup Com_ReceiveSignalGroup

#define Rte_Call_Bsw_ALIGNED_TYPE ALIGNED_TYPE
#define Rte_Call_Bsw_OSMEMORY_IS_READABLE OSMEMORY_IS_READABLE
#define Rte_Call_Bsw_OSMEMORY_IS_WRITEABLE OSMEMORY_IS_WRITEABLE
[/template]

[template private genIocApiAliasDefinitions (aHeader : RteBswApiHeader) ]
[for (aApi : IocApi | aHeader.iocApi)]
	[aApi.genIocApiAlias()/]
[/for]
[/template]

[template private genIocApiAlias(aApi : IocApi)]
#define [aApi.symbolName/] [aApi.mappingName/]
[/template]
