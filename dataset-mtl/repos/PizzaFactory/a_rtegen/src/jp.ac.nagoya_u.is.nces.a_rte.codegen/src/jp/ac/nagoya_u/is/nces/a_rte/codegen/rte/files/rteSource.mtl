[template private genTrustedFunction(aTrustedFunction : TrustedFunction) /]

[template private genTrustedFunction(aTrustedFunction : ComSendSignalTrustedFunction) ]
[aTrustedFunction.returnType.symbolName/]
[aTrustedFunction.symbolName/]([aTrustedFunction.genFunctionParams()/]) /* MISRA RULE 16.7 VIOLATION : It can't be fixed because the type(TrustedFunctionParameterRefType) is a pointer typedef */
{
	[aTrustedFunction.localVariable.type.symbolName/] *p_tfn_prm = ([aTrustedFunction.localVariable.type.symbolName/] *) tfn_prm; /* MISRA RULE 11.4 VIOLATION : It is safe because the argument variable(tfn_prm) is passed from RTE codes */
	p_tfn_prm->tf_ercd = [aTrustedFunction.signalGroupSymbolName/]([if (aTrustedFunction.isGroup)]p_tfn_prm->meta, (const uint8*)p_tfn_prm->com_data[else]p_tfn_prm->com_sig_id, p_tfn_prm->com_data[/if]); /* MISRA RULE 11.4 VIOLATION : It is safe because the argument variable(tfn_prm) is passed from RTE codes */
	if (p_tfn_prm->tf_ercd == E_OK) {
		/* Nothing to do */
	}
	else if (p_tfn_prm->tf_ercd == COM_SERVICE_NOT_AVAILABLE) {
		p_tfn_prm->tf_ercd = RTE_E_COM_STOPPED;
	}
	else {
		p_tfn_prm->tf_ercd = RTE_E_DEV_DEFECT;	
	}
	return(E_OK);
}
[/template]

[template private genTrustedFunction(aTrustedFunction : RteBufferWriteTrustedFunction) ]
[aTrustedFunction.returnType.symbolName/]
[aTrustedFunction.symbolName/]([aTrustedFunction.genFunctionParams()/]) /* MISRA RULE 16.7 VIOLATION : It can't be fixed because the type(TrustedFunctionParameterRefType) is a pointer typedef */
{
	[if (aTrustedFunction.operation.accessVariable.valueVariable.type.oclIsKindOf(PrimitiveType) or aTrustedFunction.operation.accessVariable.valueVariable.type.oclIsKindOf(PointerType))]
		[aTrustedFunction.genLocalVariableDeclarations()/]
		Rte_NonqueuedSendTrustedFunctionParamType *p_tfn_prm;
	
		p_tfn_prm = (Rte_NonqueuedSendTrustedFunctionParamType *) tfn_prm; /* MISRA RULE 11.4 VIOLATION : It is safe because the argument variable(tfn_prm) is passed from RTE codes */
	
		[aTrustedFunction.genLocalVariableInitializations()/]
	
		[aTrustedFunction.dataVariable.genValueAccess()/] = *(([aTrustedFunction.dataVariable.type.symbolName/] *) p_tfn_prm->tf_data); /* MISRA RULE 11.4 VIOLATION : It is safe because the argument variable(tfn_prm) is passed from RTE codes */
	
		[aTrustedFunction.operation.genSendOperation()/]
	[else]
		Rte_NonqueuedSendTrustedFunctionParamType *p_tfn_prm;
	
		p_tfn_prm = (Rte_NonqueuedSendTrustedFunctionParamType *) tfn_prm; /* MISRA RULE 11.4 VIOLATION : It is safe because the argument variable(tfn_prm) is passed from RTE codes */
	
		[let aExcludeOperation : ExcludeOperation = aTrustedFunction.operation.excludeOperation]
			[aExcludeOperation.genExcludeOperationEnter()/]
		[/let]
		[if (aTrustedFunction.operation.accessVariable.valueVariable.type.oclIsKindOf(ArrayType))]
			Rte_ArrayMemcpy([aTrustedFunction.operation.accessVariable.valueVariable.symbolName/], ([aTrustedFunction.operation.accessVariable.valueVariable.type.originalTypeSymbolName/] *)p_tfn_prm->tf_data, [aTrustedFunction.operation.accessVariable.valueVariable.type.symbolName/]); /* MISRA RULE 11.4 VIOLATION : It is safe because the variable is managed by RTE codes */
		[elseif (aTrustedFunction.operation.accessVariable.valueVariable.type.oclIsKindOf(StructType))]
			Rte_StructMemcpy(&[aTrustedFunction.operation.accessVariable.valueVariable.symbolName/], p_tfn_prm->tf_data, [aTrustedFunction.operation.accessVariable.valueVariable.type.symbolName/]); /* MISRA RULE 11.4 VIOLATION : It is safe because the variable is managed by RTE codes */
		[elseif (aTrustedFunction.operation.accessVariable.valueVariable.type.oclIsKindOf(UnionType))]
			Rte_UnionMemcpy(&[aTrustedFunction.operation.accessVariable.valueVariable.symbolName/], p_tfn_prm->tf_data, [aTrustedFunction.operation.accessVariable.valueVariable.type.symbolName/]); /* MISRA RULE 11.4 VIOLATION : It is safe because the variable is managed by RTE codes */
		[/if]
		[if (not aTrustedFunction.operation.accessVariable.statusVariable.oclIsUndefined())]
			[aTrustedFunction.operation.accessVariable.statusVariable.symbolName/] = [aTrustedFunction.operation.sendStatus.genValueAccess()/];
		[/if]
		[let aExcludeOperation : ExcludeOperation = aTrustedFunction.operation.excludeOperation]
			[aExcludeOperation.genExcludeOperationExit()/]
		[/let]
	[/if]
	[if (not aTrustedFunction.operation.filterOperation.oclIsUndefined())]
		p_tfn_prm->tf_rx_res = [aTrustedFunction.operation.filterOperation.filterResultVariable.genValueAccess()/];
	[else]
		p_tfn_prm->tf_rx_res = TRUE;
	[/if]

	return(E_OK);
}
[/template]

[template private genTrustedFunction(aTrustedFunction : RteBufferInvalidateTrustedFunction) ]
[aTrustedFunction.returnType.symbolName/]
[aTrustedFunction.symbolName/]([aTrustedFunction.genFunctionParams()/]) /* MISRA RULE 16.7 VIOLATION : It can't be fixed because the type(TrustedFunctionParameterRefType) is a pointer typedef */
{	
	[aTrustedFunction.genLocalVariableDeclarations()/]
	Rte_NonqueuedSendTrustedFunctionParamType *p_tfn_prm;

	p_tfn_prm = (Rte_NonqueuedSendTrustedFunctionParamType *) tfn_prm; /* MISRA RULE 11.4 VIOLATION : It is safe because the argument variable(tfn_prm) is passed from RTE codes */
	[aTrustedFunction.genLocalVariableInitializations()/]

	[aTrustedFunction.operation.genSendOperation()/]
	[if (not aTrustedFunction.operation.filterOperation.oclIsUndefined())]
		p_tfn_prm->tf_rx_res = [aTrustedFunction.operation.filterOperation.filterResultVariable.genValueAccess()/];
	[else]
		p_tfn_prm->tf_rx_res = TRUE;
	[/if]

	return(E_OK);
}
[/template]

[template private genTrustedFunction(aTrustedFunction : RteBufferSendTrustedFunction) ]
[aTrustedFunction.returnType.symbolName/]
