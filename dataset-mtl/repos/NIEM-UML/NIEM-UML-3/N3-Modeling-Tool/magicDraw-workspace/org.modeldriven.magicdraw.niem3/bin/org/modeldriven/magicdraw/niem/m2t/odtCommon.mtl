[template public propModifiers(sf : Property)]
	{[if(sf.isUnique)]unique[else]nonunique[/if]
			[if(sf.isReadOnly)] ,readOnly[/if]
			[if(sf.isDerivedUnion)] ,union[/if]
			[if(sf.isOrdered)] ,ordered[/if]
			[if(sf.isComposite)] ,composite[/if]
			[sf.subsets()/]
			[sf.redefines()/]
	}
[/template]
