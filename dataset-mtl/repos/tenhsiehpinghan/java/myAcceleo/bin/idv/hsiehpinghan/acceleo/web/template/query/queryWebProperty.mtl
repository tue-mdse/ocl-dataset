[module queryWebProperty('http://www.eclipse.org/uml2/4.0.0/UML')]

[import idv::hsiehpinghan::acceleo::utility::propertyUtility /]

[**
 * 取得是否使用struts2。
 * @param anOclAny
 */]
[comment][query public queryIsStruts2Active(anOclAny : OclAny) : Boolean = 
	(queryProperty('main', 'isWebActive') = 'Y') and (queryProperty('web', 'type') = 'struts2')
/][/comment]