[comment encoding = UTF-8 /]
[module generateResources('http://thwack.net/model/apiblueprint')]
[import net::thwack::apiblueprint::acceleo::files::generate-resource]

[template public generateElement(blueprint : APIBlueprint, javaPackage: String)]
[comment @main/]
[for (resourceGroup : ResourceGroup | blueprint.resourceGroups)]
	[for (resource : Resource | resourceGroup.resources)]
		[resource.generateElement(javaPackage)/]
	[/for]
[/for]
[/template]
