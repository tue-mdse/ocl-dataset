[module generateVision('http://www.mis.ugent.be/bmm_eml')]

[template public generateVision(aVision : Vision, aBMM : BMMmodel)]
<owl:NamedIndividual rdf:about="&[aBMM.ontologyNS/];[aVision.name/]">
	<rdf:type rdf:resource="&BMM;Vision"/>
    <rdfs:comment rdf:datatype="&xsd;string">[aVision.description/]</rdfs:comment>
 </owl:NamedIndividual>
[/template]
