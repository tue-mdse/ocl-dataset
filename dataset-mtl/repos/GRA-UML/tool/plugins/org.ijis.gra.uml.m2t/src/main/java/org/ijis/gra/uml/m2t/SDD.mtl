					<w:i />
					<w:color w:val="1F497D" />
					<w:sz w:val="24" />
					<w:szCs w:val="24" />
				</w:rPr>
			</w:pPr>
		</w:p>
		<w:p w:rsidR="0081553A" w:rsidRPr="00B82025" w:rsidRDefault="0081553A"
			w:rsidP="00B82025">
			<w:pPr>
				<w:keepNext />
