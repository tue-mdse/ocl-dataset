<html xmlns="http://www.w3.org/1999/xhtml" xmlns:rddl="http://www.rddl.org/"
      xml:lang="en">
   <head>
      <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
      <title>Service Specification Catalog</title>
      <link href="http://www.rddl.org/xrd.css" type="text/css" rel="stylesheet" />
   </head>
   <body>
      <h1>[Service_Name/] [Service_Abbreviation/] [Service_Version/]<br />Service Specification </h1>
      <h3>Summary</h3>
      <p>[Summary/]</p>
      <h3>Description</h3>
      <p>[Description/]</p>
      <h2></h2>
      <table>
         <tbody>
            <tr>
               <th align="left" valign="top">URI: </th>
               <td>[Service_URI/]</td>
            </tr>
            <tr>
               <th align="left" valign="top">Creation Date: </th>
               <td>[creationDate/]</td>
            </tr>
            <tr>
               <th align="left" valign="top">Status: </th>
               <td>[lifecycleStatus/]</td>
            </tr>
         </tbody>
      </table>
      <h2>Catalog Files</h2>
      <div class="resource">
         <ul>
            <li><a href="catalog.html"
                  rddl:purpose="http://niem.gov/niem/resources/purposes#catalog">Catalog</a></li>
            <li><a href="metadata.xml"
                  rddl:purpose="http://niem.gov/niem/resources/purposes#metadata">Metadata</a></li>
         </ul>
      </div>
      <h2>Service Description Documentation</h2>
      <div class="resource">
         <ul>
            <li><a href="[SDDDocumentLocation/]"
                  rddl:purpose="http://niem.gov/niem/resources/purposes#master-documentation">Service Description Document</a></li>
         </ul>
      </div>
      <h2>Service Interface Documentation</h2>


      <div class="resource">
         <ul>
[for(si:Interface|serviceInterfaceSpecifications)]
            <li><a href="artifacts/[Service_Abbreviation+'_SIDD_v_'+Service_Version+'.docx'/]"
                  rddl:purpose="http://niem.gov/niem/resources/purposes#master-documentation">[si.uRIDescription(ServiceInterfaceSpecificationStereotype)/] Service Interface Description Document</a></li>
[/for]
         </ul>
      </div>
      <h2>Schema and Sample Files</h2>
      <h3>&nbsp;&nbsp;&nbsp; Web Service</h3>
      <div class="resource">
         <ul>
[for(wsdl:Artifact|wsdlDefinitions)]
            <li><a href="[wsdl.pathRelativeTo(sspPackage)/].wsdl"
                  rddl:purpose="http://niem.gov/niem/resources/purposes#exchange-schema">[wsdl.name/]</a></li>
[/for]

<!-- TODO
            <li><a href="#"
                  rddl:purpose="http://niem.gov/niem/resources/purposes#exchange-schema">Web Service Sample Request</a></li>
            <li><a href="#"
                  rddl:purpose="http://niem.gov/niem/resources/purposes#exchange-schema">Web Service Sample Reply</a></li>
-->
         </ul>
      </div>
      <h3>&nbsp;&nbsp;&nbsp; Information</h3>
		<div class="resource">
         <ul>
[for(mpd:Component|mpdComponents)]
            <li><a href="[mpd.getNearestPackage().pathRelativeTo(sspPackage)/]/catalog.xml"
                  rddl:purpose="http://niem.gov/niem/resources/purposes#exchange-schema">Link to IEPD Catalog</a></li>
[/for]
         </ul>
      </div>


      <h2>Organization and Point of Contact</h2>
[for(org:Actor|organizations)]
      <table>
         <tbody>
            <tr>
               <th align="left" valign="top">Organization Name: </th>
               <td>[org.name/]</td>
            </tr>
            <tr>
               <th align="left" valign="top">Point of Contact Name: </th>
               <td>[org.contactPersonName(organizationStereotype)/]</td>
            </tr>
            <tr>
               <th align="left" valign="top">Phone: </th>
               <td>[org.contactPersonPhoneNumberID(organizationStereotype)/]</td>
            </tr>
            <tr>
               <th align="left" valign="top">Email: </th>
               <td><a href="[org.contactPersonEmailID(organizationStereotype)/]">[org.contactPersonEmailID(organizationStereotype)/]</a></td>
            </tr>
         </tbody>
      </table>
		<br/>
[/for]
   </body>
</html>
