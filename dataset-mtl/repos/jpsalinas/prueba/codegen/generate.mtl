[comment encoding = UTF-8 /]
[module generate('http://www.eclipse.org/uml2/5.0.0/UML')]


[template public generate(pa:Package)]
[comment @main/]
[file ('/documento.html', false, 'UTF-8')]
<html>
	<body>
		<ul>
			[pa.packagedElement->filter(Class).generarNombre()/]
		</ul>
	</body>
<html>


[/file]
[/template]

[template public generarNombre(c : Class)]
<li>[c.name/]</li>
[/template]
